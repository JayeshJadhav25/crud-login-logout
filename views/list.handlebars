<div class="row">
        <div class="col-md-9 offset-md-2" style="background-color: #fff;margin-top: 25px;padding:20px;">
<h3><a class="btn btn-secondary" href="/student"><i class="fa fa-plus"></i> Create New</a> Student List</h3>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Full Name</th>  
            <th>Qualification</th>
            <th>Email</th>
            <th>City</th>
        </tr>
    </thead>
      <tbody>
          {{!-- {{result}} --}}
        {{#each result}}
        <tr>
            <td>{{this.name}}</td>
            <td>{{this.Qualification}}</td>
            <td>{{this.email}}</td>
            <td>{{this.city}}</td>
            <td></td>
          <td>
                <a href="/student/{{this._id}}"><i class="fa fa-pencil fa-lg" aria-hidden="true">Update</i></a>&nbsp; &nbsp;
                <a href="/delete/{{this._id}}" onclick="return confirm('Are you sure to delete this record ?');"><i class="fa fa-trash fa-lg" aria-hidden="true">Delete</i></a>
            </td>
        </tr>

        {{/each}}
    </tbody>
</table>

    </div>